// *****************************************************************************************
// 
// templates/index.*.hbs
// 
// It runs at the root folder (ie apis-dotnet/customer) so the 'children' are the subfolders
//
// *****************************************************************************************

{{#each children}}
import * as {{name.upperCamelCase}} from "./{{name.lowerCamelCase}}/{{name.lowerCamelCase}}";
{{/each}}
export {
{{#each children}}
    {{name.upperCamelCase}}{{#unless @last}},{{/unless}}
{{/each}}
}

import { slasHelpers, helpers } from "./helpers"

export {
    slasHelpers,
    helpers
}

export { ClientConfig, CommonParameters, sdkLogger } from "@commerce-apps/core"
